.generally {
  position: relative;
  background: linear-gradient(180deg, rgb(255, 255, 255,.1) ,transparent), url(22cb5fe4aaec33b26a609208dae0ad06);
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
  color: #000;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items:center;
  padding-top: 4rem;
  width: 100%;
