.priglos {
  position: relative;
  min-width: 320px;
  display: flex;
  justify-content: center;
  align-items: center;
  backdrop-filter:blur(1px);
  background: rgba(238, 209, 168, 0.404) ;
  /* border-radius: 15px; */
  background-size: 50% 100%;
  /* border:1px solid rgba(68, 65, 65, 0.048); */
  box-shadow: 0 25px 50px rgba(0,0,0,.2);
  overflow: hidden;
}

.priglos > div {
  width: 20%;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
}

.priglos > div {
  width: 50%;
  height: 100%;
  background: url('fbfcf4ae56beeae509620a0cb27a4fa5') no-repeat 50% 50%;
  transition: all 1s ease-out;
}

.focus {
  transition: all 1s ease;
  animation: focus 3s;
  transform: scale(1);
}

@keyframes focus {
  0% {
    background-size:0%;
  }
  15% {
    background-size:15%;
  }
  50% {
    background-size:50%;
  }
  100% {
    background-size:100%;
  }
}
.priglos__title {
  width:100%;
  font-family: "Astoria";
  line-height: 1.1;
  text-align: center;
  text-indent: 1rem;
  transform: rotate(-10deg);
  font-size: 3rem;
  color: black;
  /* color:rgba(158, 158, 158, 0.2); */
  z-index: 100;
}


.priglos__info {
  font-family: "Astoria";
  line-height: 1.1;
  text-align: center;
  text-indent: 1rem;
  transform: scale(0.8);
  font-size: 3rem;
  color: black;
  z-index: 100;
}

.priglos::before {
  z-index: 1;
  position: absolute;
  content: '';
  left: 0;
  width: 100%;
  height: 100%;
  backdrop-filter:hue-rotate(0deg);
  background:  url(1eb878ac19dac55267225c1818afd02c) no-repeat 0% 100%, transparent; 
  background-size: 30%;
  animation: colorPick 30s infinite;
}

.priglos::after {
  z-index: 1;
  position: absolute;
  content: '';
  right: 0;
  width: 100%;
  height: 100%;
  background:  url(9cbeabc67882ff22059341d17d2f0d00) no-repeat 100% 0%;
  background-size: 30%;
}

@keyframes colorPick {
  0%{
    backdrop-filter:hue-rotate(0deg);
  }
   50% {
    backdrop-filter:hue-rotate(720deg);
  }
  100%{
    backdrop-filter:hue-rotate(0deg);
    
  }
}

@keyframes colorHello {
  0% {
    background-color:var(--pink);
  }
  15% {
    background-color:var(--crem);
  }
  50% {
    background-color:var(--gar);
  }
  100% {
    background-color: var(--pink)
  }
}
