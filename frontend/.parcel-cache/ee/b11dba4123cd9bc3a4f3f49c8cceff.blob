.priglos {
  position: relative;
  min-width: 320px;
  width: 50vw;
  height: 50vh;
  display: flex;
  justify-content: center;
  align-items: center;
  backdrop-filter:blur(1px);
  background: rgba(238, 209, 168, 0.404) ;
  border-radius: 15px;
  background-size: 50% 100%;
  border:1px solid rgba(68, 65, 65, 0.048);
  box-shadow: 0 25px 50px rgba(0,0,0,.2);
  overflow: hidden;
}

.priglos > div {
  width: 20%;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
}

.priglos > div {
  width: 50%;
  height: 100%;
  background: url('data:image/svg+xml,\<svg viewBox="0 -28 512.00002 512" xmlns="http://www.w3.org/2000/svg"><path fill="rgba(255,255,255, .4)" d="m471.382812 44.578125c-26.503906-28.746094-62.871093-44.578125-102.410156-44.578125-29.554687 0-56.621094 9.34375-80.449218 27.769531-12.023438 9.300781-22.917969 20.679688-32.523438 33.960938-9.601562-13.277344-20.5-24.660157-32.527344-33.960938-23.824218-18.425781-50.890625-27.769531-80.445312-27.769531-39.539063 0-75.910156 15.832031-102.414063 44.578125-26.1875 28.410156-40.613281 67.222656-40.613281 109.292969 0 43.300781 16.136719 82.9375 50.78125 124.742187 30.992188 37.394531 75.535156 75.355469 127.117188 119.3125 17.613281 15.011719 37.578124 32.027344 58.308593 50.152344 5.476563 4.796875 12.503907 7.4375 19.792969 7.4375 7.285156 0 14.316406-2.640625 19.785156-7.429687 20.730469-18.128907 40.707032-35.152344 58.328125-50.171876 51.574219-43.949218 96.117188-81.90625 127.109375-119.304687 34.644532-41.800781 50.777344-81.4375 50.777344-124.742187 0-42.066407-14.425781-80.878907-40.617188-109.289063zm0 0"/></svg>') no-repeat 50% 50%;
  transition: all 1s ease-out;
}

.focus {
  transition: all 1s ease;
  animation: focus 3s;
  transform: scale(1);
}

@keyframes focus {
  0% {
    background-size:0%;
  }
  15% {
    background-size:15%;
  }
  50% {
    background-size:50%;
  }
  100% {
    background-size:100%;
  }
}
.priglos__title {
  width:100%;
  font-family: "Astoria";
  line-height: 1.1;
  text-align: center;
  text-indent: 1rem;
  transform: rotate(-10deg);
  font-size: 3rem;
  color: black;
  /* color:rgba(158, 158, 158, 0.2); */
  z-index: 100;
}


.priglos__info {
  font-family: "Astoria";
  line-height: 1.1;
  text-align: center;
  text-indent: 1rem;
  transform: scale(0.8);
  font-size: 3rem;
  color: black;
  z-index: 100;
}

.priglos::before {
  z-index: 1;
  position: absolute;
  content: '';
  left: 0;
  width: 100%;
  height: 100%;
  backdrop-filter:hue-rotate(0deg);
  background:  url(h1-slider-1-img-44.a857cf26.png) no-repeat 0% 100%, transparent; 
  background-size: 30%;
  animation: colorPick 30s infinite;
}

.priglos::after {
  z-index: 1;
  position: absolute;
  content: '';
  right: 0;
  width: 100%;
  height: 100%;
  background:  url(h1-slider-1-img-2.0c6d5999.png) no-repeat 100% 0%;
  background-size: 30%;
}

@keyframes colorPick {
  0%{
    backdrop-filter:hue-rotate(0deg);
  }
   50% {
    backdrop-filter:hue-rotate(720deg);
  }
  100%{
    backdrop-filter:hue-rotate(0deg);
    
  }
}

@keyframes colorHello {
  0% {
    background-color:var(--pink);
  }
  15% {
    background-color:var(--crem);
  }
  50% {
    background-color:var(--gar);
  }
  100% {
    background-color: var(--pink)
  }
}
