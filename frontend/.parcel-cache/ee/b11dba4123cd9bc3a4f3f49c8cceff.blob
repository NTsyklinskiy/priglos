* {
  padding: 0px;
  margin: 0px;
  border: 0px;
}

*,
*:before,
*:after {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

:focus,
:active {
  outline: none;
}

a:focus,
a:active {
  outline: none;
}

aside,
nav,
footer,
header,
section {
  display: block;
}

html,
body {
  height: 100%;
  margin: 0;
  padding: 0;
  min-width: 320px;
  position: relative;
  width: 100%;
  color: #000;
}

body {
  font-size: 100%;
  font-size: 14px;
  -ms-text-size-adjust: 100%;
  -moz-text-size-adjust: 100%;
  -webkit-text-size-adjust: 100%;
}



input,
button,
textarea {
  font-family: "";
}

input::-ms-clear {
  display: none;
}

button {
  cursor: pointer;
}

button::-moz-focus-inner {
  padding: 0;
  border: 0;
}

a,
a:visited {
  text-decoration: none;
}

a:hover {
  text-decoration: none;
}

ul li {
  list-style: none;
}

img {
  vertical-align: top;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-weight: inherit;
  font-size: inherit;
}

:root {
  --pink-light: #f9e9ed;
  --crem:rgb(223, 201, 171);
  --gar: rgb(189, 186, 112);
  --pink: rgb(251,185,173);

  /* background:  width: 100%; height: 100%; opacity: 1; visibility: inherit; z-index: 20; */
}

html {
  font-size: 62.5%;
}

body{
  font-family: 'Conv';
  font-size: 15px;
  line-height: 25px;
  color: #fff;
  
}

.wrapper {
  position: relative;
  max-width: 100%;
  width: 100%;
  min-height: 100vh;
  min-width: 320px;
  overflow: hidden;
}

.container {
  max-width: 1170px;
  margin: 0 auto;
  width: 100%;
  min-width: 320px;
  padding: 0 15px;
  margin: 0 auto;
}

@media (max-width: 1182px) {
  .container {
    max-width: 970px;
    height: 100vh;
  }
  
}

@media (max-width: 992.98px) {
  .container {
    max-width: 750px;
  }
}

@media (max-width: 767.98px) {
  .container {
    max-width: none;
    padding: 0 10px;
  }
}

.header {
  position: absolute;
  width: 100%;
  padding:1.5rem;
  z-index: 1000;

  background: linear-gradient(rgba(240,237,232,.44), transparent);
  backdrop-filter: blur(1px)
}

.navigation {
  position: sticky;
  font-size: 1.5rem;
  display: flex;
  justify-content: space-between;
}

.navigation__items {
  width: 80%;
  margin-left: 15%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-evenly;
}

.navigation__link {
  background: transparent;
  font-size: 2.5rem;
  font-weight: 500;
  /* backdrop-filter:blur(1px) ; */
  /* filter: blur(1px); */
  color: rgb(255, 255, 255);
  transition: all 0.4s ease-out;
}

.hello {
  background: url(h1-slider-1-img-3.d6d82c87.png) no-repeat right, url(h1-slider-1-img-2.0c6d5999.png) no-repeat  right top , url(h1-slider-1-img-4.fb9e9e97.png) no-repeat, url(h1-slider-1-img-5.6283f475.png) no-repeat bottom right, url(h1-slider-1-img-6.94a26708.png) no-repeat 50% 100%, url(h1-slider-1-img-7.92a119b7.png) no-repeat bottom left;
  background-size: 10%, 35%, 35%, 15%, 25%, 20%;
  /* background-color: var(--pink);  */
  width: 100%;
  min-height: 100vh;
  overflow: hidden;
  animation: colorHello 10s infinite;
}

.hello > .container {
  height: 100vh;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  position: relative;
}

.hello__contain{
  position: relative;
  width:350px;
  height:350px;
  box-shadow: 0 25px 50px rgba(0,0,0,.2);
  background: rgba(158, 158, 158, 0.2);
  backdrop-filter: blur(1px);
  border: 0.1px solid rgb(223, 201, 171, .1);
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  z-index: 100;
}

 .first {
  position: absolute;
  top:15%;
  font-size: 6rem;
  /* text-transform: uppercase; */
  font-weight: 900;
  width:100rem;
  height: 10rem;
  padding: 4rem;
  background: linear-gradient(transparent 10%,rgba(255, 255, 255, 0.6)15%, transparent) ;
  background-size:cover;
  background-attachment: fixed;
  -webkit-background-clip: text;
  z-index: 2;
  -webkit-text-fill-color: transparent;
  
}
 .first::before {
  content: 'Мы станем семьёй';
  text-shadow: 1px 1px 6px rgba(240,237,232,.44) ;
  width: 200%;
  color:transparent;
  z-index: 1;
  position: absolute;
  font-weight: 900;
}

.second {
  position: absolute;
  bottom:15%;
  left: 45%;
  font-size: 6rem;
  /* text-transform: uppercase; */
  font-weight: 900;
  width:100rem;
  height: 10rem;
  padding: 4rem;
  background: linear-gradient(transparent 10%,rgba(255, 255, 255, 0.6)45%) ;
  background-size:cover;
  background-attachment: fixed;
  -webkit-background-clip: text;
  z-index: 2;
  -webkit-text-fill-color: transparent;
}
.second::before {
  position: absolute;
  content: '10 июля 2021 года';
  text-shadow: 1px 1px 6px rgba(240,237,232,.44) ;
  width: 200%;
  color:transparent;
  z-index: 1;
  font-weight: 900;
}


.hello__name {
  margin: 0 0 1rem 0;
  font-size: 2.5rem;
  font-weight: 500;
}
.hello__logg {
  position: relative;
  font-family: "Astoria";
  font-weight: 400;
  font-size: 5.5rem;
  margin: 0 0 1rem 0;
  
}
.hello__logg::before {
  content: "";
  width: 250px;
  height: 1px;
  background-color: #ffffff;
  position: absolute;
  left: 50%;
  bottom: -5px;
  transform: translateX(-50%);
  transition-duration: 0.3s;
}

#timer {
display: flex;
justify-content: center;
align-items: center;
/* margin: 1.5rem; */
}

#timer > div {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
  font-size: 2.5rem;
  /* padding: 4px 4px; */
  width: 5rem;
  height: 5rem;
  margin: 1rem;
  background: transparent;
  border: 1px solid rgb(255, 255, 255);
  border-radius:50%;
  text-align: center;
}

#timer > div::after{
  content: "";
  width: 35px;
  height: 1px;
  background-color: #ffffff;
  position: absolute;
  left: 50%;
  bottom: 21px;
  transform: translateX(-50%);
  transition-duration: 0.3s; 
}

#timer > div:not(:last-child)::before{
  content: ":";
  position: absolute;
  top: 20%;
  right: -30%;
  transition-duration: 0.3s;
}

#timer > div > span {
  transform: translate(1px,3px);
}



.hello::after {
  content: '';
  position: absolute;
  top: 29%;
  backdrop-filter: blur(.5px);
  background: linear-gradient( 0deg,rgba(240,237,232,.44) , transparent );
  width: 100%;
  height: 16vh;
  z-index: 100;
}

.invitation {
  height: 100vh;
  min-height: 60rem;
  background:  linear-gradient(#f0ede8ef, #f0ede86c) ;
  /* filter: blur(8px); */
  background-size: cover;
  overflow: hidden;
  display: flex;
  justify-content: center;
  align-items:center;
  /* z-index: 101; */
}
