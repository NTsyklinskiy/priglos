@charset "UTF-8";

@font-face {
	font-family: 'Conv';
	src: url('13356.a5b41f6c.eot');
	src: local('☺'), url('13356.17aa14ba.woff') format('woff'), url('13356.a684df1e.ttf') format('truetype'), url('13356.eba25ec5.svg') format('svg');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'Astoria';
	src: url('Astoria.544e6433.eot');
	src: local('☺'), url('Astoria.6207397c.woff') format('woff'), url('Astoria.1d488416.ttf') format('truetype'), url('Astoria.244fd040.svg') format('svg');
