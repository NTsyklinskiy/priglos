.header {
  position: fixed;
  width: 100%;
  font-size: 3rem;
  padding:1.5rem;
  z-index: 3;
  background: linear-gradient(rgba(240,237,232,.44), transparent);
  backdrop-filter: blur(1px)
}

.navigation {
  position: sticky;
  display: flex;
  justify-content: space-between;
}

.navigation__items {
  font-size: 3rem;
  width: 80%;
  margin-left: 15%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-evenly;
}

.navigation__link {
  background: transparent;
  font-size: 3rem;
  font-weight: 500;
  /* backdrop-filter:blur(1px) ; */
  /* filter: blur(1px); */
  color: rgb(255, 255, 255);
  transition: all 0.4s ease-out;
}

.main {
  background: url(landing-section1-background-img.a1d5d033.png) no-repeat 10% 20%,url(h5-slide-1-img-1.80317d11.png) no-repeat top, url(h1-slider-1-img-2.b917187e.png) no-repeat  right top , url(h1-slider-1-img-4.c585c462.png) no-repeat, url(h1-slider-1-img-41.d81c76c1.png) no-repeat bottom right, url(h1-slider-1-img-6.f34209b8.png) no-repeat 50% 100%, url(h1-slider-1-img-22.83c5b75a.png) no-repeat bottom left;
  background-size:100%,25%, 35%, 35%,35%, 25%, 35%;
  animation: colorHello 10s infinite;

}


.hello {
  width: 100%;
  min-height: 100vh;
  backdrop-filter:blur(1px);
}

.hello > .container {
  height: 100vh;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  position: relative;
}

.hello__contain{
  position: relative;
  width:350px;
  height:350px;
  box-shadow: 0 25px 50px rgba(0,0,0,.2);
  background: rgba(158, 158, 158, 0.2);
  backdrop-filter: blur(1px);
  border: 0.1px solid rgb(223, 201, 171, .1);
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  z-index: 3;
}

 .first {
  position: absolute;
  top:15%;
  left: 5%;
  font-size: 6rem;
  /* text-transform: uppercase; */
  font-weight: 900;
  width:100rem;
  height: 10rem;
  padding: 4rem;
  background: linear-gradient(transparent 10%,rgba(255, 255, 255, 0.6)15%, transparent) ;
  background-size:cover;
  background-attachment: fixed;
  -webkit-background-clip: text;
  z-index: 2;
  -webkit-text-fill-color: transparent;
  transition: all 1s ease-out;
  animation: edemL 10s;
}

@keyframes edemL {
  0% {
    left: 150%;
  } 100% {
    left: 5%;
  }
}

 .first::before {
  content: 'Мы станем семьёй';
  text-shadow: 1px 1px 6px rgba(240,237,232,.44) ;
  width: 200%;
  color:transparent;
  z-index: 1;
  position: absolute;
  font-weight: 900;
}

.second {
  position: absolute;
  bottom:15%;
  left: 45%;
  font-size: 6rem;
  /* text-transform: uppercase; */
  font-weight: 900;
  width:100rem;
  height: 10rem;
  padding: 4rem;
  background: linear-gradient(transparent 10%,rgba(255, 255, 255, 0.6)45%) ;
  background-size:cover;
  background-attachment: fixed;
  -webkit-background-clip: text;
  z-index: 2;
  -webkit-text-fill-color: transparent;
  transition: all 1s ease-out;
  animation: edem 10s;
}


@keyframes edem {
  0% {
    left: -100%;
  } 100% {
    left: 45%;
  }
}

.second::before {
  position: absolute;
  content: '10 июля 2021 года';
  text-shadow: 1px 1px 6px rgba(240,237,232,.44) ;
  width: 200%;
  color:transparent;
  z-index: 1;
  font-weight: 900;
}


.hello__name {
  margin: 0 0 1rem 0;
  font-size: 2.5rem;
  font-weight: 500;
}
.hello__logg {
  position: relative;
  font-family: "Astoria";
  font-weight: 400;
  font-size: 5.5rem;
  margin: 0 0 1rem 0;
  
}
.hello__logg::before {
  content: "";
  width: 250px;
  height: 1px;
  background-color: #ffffff;
  position: absolute;
  left: 50%;
  bottom: -5px;
  transform: translateX(-50%);
  transition-duration: 0.3s;
}

#timer {
display: flex;
justify-content: center;
align-items: center;
/* margin: 1.5rem; */
}

#timer > div {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
  font-size: 2.5rem;
  /* padding: 4px 4px; */
  width: 5rem;
  height: 5rem;
  margin: 1rem;
  background: transparent;
  border: 1px solid rgb(255, 255, 255);
  border-radius:50%;
  text-align: center;
}

#timer > div::after{
  content: "";
  width: 35px;
  height: 1px;
  background-color: #ffffff;
  position: absolute;
  left: 50%;
  bottom: 21px;
  transform: translateX(-50%);
  transition-duration: 0.3s; 
}

#timer > div:not(:last-child)::before{
  content: ":";
  position: absolute;
  top: 20%;
  right: -30%;
  transition-duration: 0.3s;
}

#timer > div > span {
  transform: translate(1px,3px);
}


