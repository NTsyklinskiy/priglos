@keyframes focus {
  0% {
    background-size:90%;
  }
  25% {
    background-size:100%;
  }
  50% {
    background-size:95%;
  }
  75% {
    background-size:100%;
  }
  100% {
    background-size:90%;
  }
}

.priglos__title {
  position: relative;
  width:50%;
  height: 75%;
  top:15%;
  font-family: "Astoria";
  line-height: 1.5;
  text-align: center;
  text-indent: 2rem;
  font-size: 3rem;
  color: black;
  transform: rotate(-20deg);
  transition: all 1s ease-out;
  z-index: 100;
}







@keyframes colorPick {
  0%{
    filter:hue-rotate(0deg);
  }
   50% {
    filter:hue-rotate(720deg);
  }
  100%{
    filter:hue-rotate(0deg);
    
  }
}

@keyframes colorHello {
  0% {
    background-color:var(--pink);
  }
  15% {
    background-color:var(--crem);
  }
  50% {
    background-color:var(--gar);
  }
  100% {
    background-color: var(--pink)
  }
}
