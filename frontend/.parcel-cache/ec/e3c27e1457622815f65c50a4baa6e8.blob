* {
  padding: 0px;
  margin: 0px;
  border: 0px;
}

*,
*:before,
*:after {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

:focus,
:active {
  outline: none;
}

a:focus,
a:active {
  outline: none;
}

aside,
nav,
footer,
header,
section {
  display: block;
}

html,
body {
  height: 100%;
  margin: 0;
  padding: 0;
  min-width: 320px;
  position: relative;
  width: 100%;
  color: #000;
}

body {
  overflow: hidden;
  font-size: 100%;
  font-size: 14px;
  -ms-text-size-adjust: 100%;
  -moz-text-size-adjust: 100%;
  -webkit-text-size-adjust: 100%;
}



input,
button,
textarea {
  font-family: "";
}

input::-ms-clear {
  display: none;
}

button {
  cursor: pointer;
}

button::-moz-focus-inner {
  padding: 0;
  border: 0;
}

a,
a:visited {
  text-decoration: none;
}

a:hover {
  text-decoration: none;
}

ul li {
  list-style: none;
}

img {
  vertical-align: top;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-weight: inherit;
  font-size: inherit;
}

:root {
  --pink-light: #f9e9ed;
  --crem:rgb(223, 201, 171);
  --gar: rgb(189, 186, 112);
  --pink: rgb(251,185,173);

  /* background:  width: 100%; height: 100%; opacity: 1; visibility: inherit; z-index: 20; */
}

html {
  font-size: 62.5%;
}

body{
  font-family: 'Conv';
  font-size: 15px;
  line-height: 25px;
  color: #fff;
  
}

.wrapper {
  position: relative;
  max-width: 100%;
  width: 100%;
  min-height: 100vh;
  min-width: 320px;
  overflow: hidden;
}

.container {
  max-width: 1170px;
  margin: 0 auto;
  width: 100%;
  min-width: 320px;
  padding: 0 15px;
  margin: 0 auto;
}

.hidden {
  overflow: hidden;
  transition: all 1s ease-out;
  opacity: 0;
}
@media (max-width: 1182px) {
  .container {
    max-width: 970px;
  }

}

@media (max-width: 992.98px) {
  .container {
    max-width: 750px;
  }
}

@media (max-width: 767.98px) {
  .container {
    max-width: none;
    padding: 0 10px;
  }

}


