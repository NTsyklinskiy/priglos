.modal {
  position: fixed;
  top: 0;
  z-index: 10;
  left: 0;
  width: 100vw;
  height: 100vh;
  display: flex;
  justify-content: center;
  align-items: center;
}
.overflow {
  width: 100%;  
  height: 100%;
  background: rgba(255, 255, 255, 0.034);
  position: absolute;
  z-index: 3;
  backdrop-filter: blur(15px);
  /* filter: blur(1); */
  
}

.modal-form {
  position: relative;
  flex-direction: column;
  border-radius: 30px;
  top: 0;
  color: rgb(85, 77, 76);
  left: 0;
  width: 40%;
  height: 60%;
  background:  rgb(255, 255, 255);
  z-index: 10;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  /* justify-content: space-evenly; */
  gap: 1.5rem;
  font-size: 2rem;
  box-shadow: 10px 10px 40px 4px rgb(0 0 0 / 40%), -10px -10px 40px 4px rgb(0 0 0 / 40%);
}

.modal-form > h2 {
  text-align: center;
  font-size: 2rem;
  padding: 3rem 5rem 0;
}



.modal-form > input {
  margin: 1rem 2rem;
  padding: 1rem 2rem;
  width: 60%;
  border: 1px solid rgba(251,185,173, 0.384);
  box-shadow: 3px 3px 3px 1px rgba(251,185,173, .4),
  -3px -3px 3px 1px rgba(251,185,173, .4);
  border-radius: 30px;
  font-size: 2rem;
  background: transparent;
}

.modal-form > input:hover,
.modal-form > button {
  box-shadow: 3px 3px 3px 1px rgba(251,185,173, .9),
  -3px -3px 3px 1px rgba(251,185,173, .9);
}
::placeholder{
  color: rgb(85, 77, 76);
}

.modal-form > button {
  width: 150px;
  height: 50px;
  color: rgb(85, 77, 76);
  box-shadow: 3px 3px 3px 1px rgba(251,185,173, .4),
  -3px -3px 3px 1px rgba(251,185,173, .4);
  border: 1px solid rgba(251,185,173, 0.37);
  border-radius: 30px;
  background: transparent; 
  font-size: 2rem;
  outline: none;
  /* justify-self: center; */
}


